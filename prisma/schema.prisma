datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  openId    String   @unique
  email     String   @unique
  firstName String
  lastName  String
  imgSrc    String
  createdAt DateTime @default(now())
}
